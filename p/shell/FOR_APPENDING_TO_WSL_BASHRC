#note the v-f below cannot yank and paste across closing/opening of vim, but the v-y can do it
alias v-f='vim -n -i NONE -u NONE "+se clipboard=unnamedplus hls ai enc=utf-8 nocompatible nu backspace=indent,eol,start" "+syntax on"'
alias v-y='vim "+se hls"'
alias v='v-g'
export "PATH=$PATH:/home/eric/go/bin:/usr/local/go/bin:/opt/u_bin"
export VISUAL=vim
export EDITOR="$VISUAL"
export DOTNET_CLI_TELEMETRY_OPTOUT=1

